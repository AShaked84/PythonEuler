   "source": [
    "def EvenFibonacciSum(maxNum):\n",
    "    sum = 0\n",
    "\n",
    "    a = 1\n",
    "    b = 2\n",
    "\n",
    "    while a <= maxNum:\n",
    "        if a%2 == 0:\n",
    "            sum += a\n",
    "        temp = a + b\n",
    "        a = b\n",
    "        b = temp\n",
    "\n",
    "    return sum"
   ]
  },
