    "def FizzBuzz(maxNum):\n",
    "    sum = 0\n",
    "\n",
    "    for i in range(maxNum):\n",
    "        if i%3 == 0 or i%5 == 0:\n",
    "            sum += i\n",
    "\n",
    "    return sum"
