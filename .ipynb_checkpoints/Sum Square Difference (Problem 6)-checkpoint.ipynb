   "source": [
    "def SumSquareDifference(maxNum):\n",
    "    sqrsum = 0\n",
    "    sumsqr = 0\n",
    "    \n",
    "    for i in range(1, maxNum+1):\n",
    "        sqrsum += i**2\n",
    "        sumsqr += i\n",
    "\n",
    "    return sumsqr**2 - sqrsum"
   ]
